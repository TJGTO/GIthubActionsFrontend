{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "unused-imports"],
  "rules": {
    "no-unused-vars": "error",
    "unused-imports/no-unused-imports": "error"
  }
}
